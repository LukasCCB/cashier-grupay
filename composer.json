{
    "name": "LukasCCB/cashier-grupay",
    "description": "Cashier GruPay provides an expressive, fluent interface to GruPay's subscription billing services.",
    "keywords": ["laravel", "grupay", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Silva",
            "email": "admin@grupay.app"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "moneyphp/money": "^3.2|^4.0",
        "nesbot/carbon": "^2.67|^3.0",
        "spatie/url": "^1.3.5|^2.0",
        "symfony/http-kernel": "^6.2|^7.0",
        "symfony/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^8.14|^9.0|^10.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4|^11.5"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "LukasCCB\\GruPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "LukasCCB": {
            "providers": [
                "LukasCCB\\GruPay\\CashierServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
